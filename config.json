{
    "models": {
        "st": {
            "wandb_project": "General-model-English",
            "wandb_run": "Sent-ST",
            "tokenizer_model": "bert-base-multilingual-cased",
            "model_input_dim": 768,
            "loss_function": "BCEWithLogitsLoss",
            "optimizer": "Adam",
            "learning_rate": 0.001,
            "dataset_path": "./combined_data/Sent-Spacy/combined_data_en.ol",
            "separator": "\t",
            "text_column": "Sentence",
            "label_column": "Label",
            "batch_size": 32,
            "num_epochs": 10,
            "early_stopping_patience": 3,
            "output_model_dir": "./Sent-ST-bert-model"
        },
        "baseline": {
            "wandb_project": "Baseline-English",
            "wandb_run": "Baseline-Run",
            "tokenizer_model": "xlm-roberta-base",
            "model_input_dim": 768,
            "loss_function": "CrossEntropyLoss",
            "optimizer": "Adam",
            "learning_rate": 0.00001,
            "dataset_path": "./data/baseline_train.csv",
            "separator": ",",
            "text_column": "text",
            "label_column": "label",
            "batch_size": 16,
            "num_epochs": 10,
            "early_stopping_patience": 3,
            "output_model_dir": "./Baseline-Model"
        },
        "label_smoothing": {
            "wandb_project": "Label-Smoothing-Model",
            "wandb_run": "Label-Smoothing-Transformer",
            "tokenizer_model": "bert-base-uncased",
            "loss_function": "CrossEntropyLoss",
            "optimizer": "Adam",
            "learning_rate": 0.00005,
            "dataset_path": "./data/label_smoothing_train.csv",
            "separator": "\t",
            "text_column": "Clean_Text",
            "label_column": "Label",
            "batch_size": 32,
            "num_epochs": 10,
            "early_stopping_patience": 3,
            "smoothing_factor": 0.1,
            "output_model_dir": "./Label-Smoothing-Model",
            "valsplit": 0.2,
            "seed": 42,
            "testfile": "./data/label_smoothing_test.csv"
        }
    }
}
